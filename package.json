{
  "name": "natural-capital-and-ecosystem-assessment",
  "version": "1.0.0",
  "description": "",
  "main": "src/index.ts",
  "scripts": {
    "build": "rimraf ./build && tsc && npm run copy-files",
    "copy-files": "ts-node copyNonTSFiles.ts",
    "start:dev": "npx nodemon --config tools/nodemon.json",
    "start": "npm run build && node build/index.js",
    "lint": "npm run lint:javascript",
    "lint:javascript": "eslint --config tools/.eslintrc src/**/* --ext .js,.ts",
    "lint:javascript:fix": "eslint --config tools/.eslintrc src/**/* --fix --ext .js,.ts",
    "sass:compile": "sass --no-source-map --load-path=node_modules/govuk-frontend/dist/ --quiet-deps src/assets/sass/_root.scss public/application.css",
    "prettier-format": "run-script-os",
    "prettier-format:win32": "prettier --config tools/.prettierrc \"./src/**/*.ts\" --write",
    "prettier-format:darwin:linux": "prettier --config tools/.prettierrc 'src/**/*.ts' --write",
    "prettier-format:default": "prettier --config tools/.prettierrc 'src/**/*.ts' --write",
    "prettier-watch": "run-script-os",
    "prettier-watch:win32": "onchange \"src/**/*.ts\" -- prettier --write {{changed}}",
    "prettier-watch:darwin:linux": "onchange 'src/**/*.ts' -- prettier --write {{changed}}",
    "prettier-watch:default": "onchange 'src/**/*.ts' -- prettier --write {{changed}}",
    "test": "jest --config tools/jest.config.js --coverage -u",
    "test:dev": "jest --watchAll --config tools/jest.config.js -u",
    "prepare": "husky install .husky"
  },
  "keywords": [],
  "engines": {
    "node": ">=16",
    "npm": ">=8"
  },
  "os": [
    "drawin",
    "linux",
    "win32"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/hapi__hapi": "21.0.0",
    "@types/hapi__vision": "5.5.7",
    "@types/jest": "29.5.11",
    "@types/node": "20.10.5",
    "@types/nunjucks": "3.2.6",
    "@types/shelljs": "0.8.15",
    "@typescript-eslint/eslint-plugin": "6.15.0",
    "@typescript-eslint/parser": "6.15.0",
    "eslint": "8.56.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-jest": "27.6.0",
    "eslint-plugin-prettier": "5.1.0",
    "husky": "8.0.3",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "jest-mock-extended": "3.0.5",
    "jest-sonar-reporter": "2.0.0",
    "nodemon": "3.0.2",
    "nunjucks-date-filter": "^0.1.1",
    "onchange": "7.1.0",
    "prettier": "3.1.1",
    "rimraf": "5.0.5",
    "run-script-os": "1.1.6",
    "sass": "1.69.7",
    "ts-jest": "29.1.1",
    "ts-node": "10.9.2",
    "typescript": "5.3.3"
  },
  "dependencies": {
    "@hapi/hapi": "21.3.2",
    "@hapi/inert": "7.1.0",
    "@hapi/vision": "7.0.3",
    "@types/hapi__joi": "^17.1.14",
    "axios": "1.6.4",
    "govuk-frontend": "5.0.0",
    "hapi-pino": "12.1.0",
    "joi": "^17.12.0",
    "nunjucks": "3.2.4",
    "shelljs": "0.8.5"
  },
  "jestSonar": {
    "reportPath": "reports",
    "reportFile": "test-report.xml",
    "indent": 4
  }
}
